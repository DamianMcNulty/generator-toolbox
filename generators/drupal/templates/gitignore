# Ignore file generated by MacOS
*.DS_Store

# Ignore directories & files generated by Styleguide
node_modules
.sass-cache
.publish
.bundle
.env
npm-debug.log
/dist
/build
/styleguide
/web/themes/[PROJECT_DRUPAL_THEME]/build

# Ignore directories & files generated by Capistrano
/log

# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.local.php
/web/sites/*/services*.yml
/web/sites/*/drushrc.php

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore files settings Drupal must not be exported
# See this issue: https://github.com/drush-ops/drush/issues/1820
/config/d8/sync/devel.*
/config/d8/sync/system.menu.devel.yml
/config/d8/sync/webprofiler.config.yml

# Ignore nohup created by Behat
web/nohup.out
# Ignore output for debugging tests
web/localhost
